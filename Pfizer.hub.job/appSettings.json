{
  "Serilog": {
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Async",  
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "restrictedToMinimumLevel": "Information",
                "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
                "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:j}{NewLine}{Properties:j}{NewLine}{Exception}"
              }
            },
            {
              "Name": "File",
              "Args": {
                "restrictedToMinimumLevel": "Information",
                "path": "./Logs/log_.log",
                "rollingInterval": "Day",
                "fileSizeLimitBytes": 50240,
                "rollOnFileSizeLimit": true,
                "outputTemplate": "{Timestamp:HH:mm} [{Level}] {MachineName} ({ThreadId}) {Message}{NewLine}{Exception}"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithExceptionDetails" ],
    "Properties": {
      "ApplicationName": "Pfizer.Hub.Job",
      "Environment": "Int"
    }
  },
  "url":{
         "LoginUrl":"https://localhost:5001/api/Account/login",
         "StockFeederUrl":"https://localhost:5001/api/StockFeeder/SendStockInformation",
         "PostStockInfoUrl": "https://localhost:5001/api/LifeSavingHub/PostStockInfo"
  },
  "pizeraccount":{
    "Username":"clientusr",
    "Password":"Aplprim4$1234567890"
  }
}